{
    "manifest_version": 3,
    "name": "TimeOut",
    "version": "1.0",
    "description": "Creates reminders to take breaks while browsing.",
    "author": ["Jakob Linenberger", "Thuan Nguyen", "Cambren Williams"], 
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png",
        "512": "images/icon512.png"
      }
    },
    "icons":{
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png",
      "512": "images/icon512.png"
    }
      
  }